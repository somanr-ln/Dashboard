<div width="250px" 
	style="background: white;"
	apply="org.hpccsystems.dashboard.controller.NumericFilterController">
	
	<listbox hflex="1" nonselectableTags="*" oddRowSclass="non-odd">
		<listhead>
	    	<listheader label="${labels.specifyRange}">
	    		<button sclass="glyphicon glyphicon-new-window btn btn-link img-btn" style="float:right" id="filtersSelectedBtn"></button>
	    	</listheader>
	    </listhead>
	    <listitem>
	    	<listcell>
	    		<label value="${labels.from}" />
	    		<label id="minimumLabel"></label>
				<slider id="minimumSlider" />
	    	</listcell>
	    </listitem>
	    <listitem>
	    	<listcell>
	    		<label value="${labels.to}" />
	    		<label id="maximumLabel"></label>
				<slider id="maximumSlider" />	
	    	</listcell>
	    </listitem>
	</listbox>
</div>